v000001a3ffc427f0_0 .var "pwm_out", 0 0;
v000001a3ffc42890_0 .net "rst", 0 0, v000001a3ffc747d0_0;  1 drivers
E_000001a3ffc28420 .event posedge, v000001a3ffc42c50_0;
    .scope S_000001a3ffc2aaa0;
T_0 ;
    %pushi/vec4 10, 0, 16;
    %store/vec4 v000001a3ffc42750_0, 0, 16;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001a3ffc42570_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001a3ffc42610_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_000001a3ffc2aaa0;
T_1 ;
    %wait E_000001a3ffc28420;
    %load/vec4 v000001a3ffc42890_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v000001a3ffc43070_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v000001a3ffc43070_0;
    %pad/u 32;
    %cmpi/u 9, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_1.2, 5;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v000001a3ffc43070_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v000001a3ffc43070_0;
    %addi 1, 0, 16;
    %assign/vec4 v000001a3ffc43070_0, 0;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_000001a3ffc2aaa0;
T_2 ;
    %wait E_000001a3ffc28420;
    %load/vec4 v000001a3ffc42890_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 10, 0, 16;
    %assign/vec4 v000001a3ffc42750_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v000001a3ffc426b0_0;
    %cmpi/u 20, 0, 16;
    %flag_or 5, 4; GT is !LE
    %flag_inv 5;
    %jmp/0xz  T_2.2, 5;
    %pushi/vec4 20, 0, 16;
    %assign/vec4 v000001a3ffc42750_0, 0;
    %jmp T_2.3;
T_2.2 ;
    %load/vec4 v000001a3ffc426b0_0;
    %assign/vec4 v000001a3ffc42750_0, 0;
T_2.3 ;
T_2.1 ;
