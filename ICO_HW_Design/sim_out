    %jmp T_3.1;
T_3.0 ;
    %load/vec4 v000001a3ffc42750_0;
    %cmpi/u 10, 0, 16;
    %jmp/0xz  T_3.2, 5;
    %pushi/vec4 10, 0, 16;
    %load/vec4 v000001a3ffc42750_0;
    %sub;
    %assign/vec4 v000001a3ffc424d0_0, 0;
    %jmp T_3.3;
T_3.2 ;
    %load/vec4 v000001a3ffc42750_0;
