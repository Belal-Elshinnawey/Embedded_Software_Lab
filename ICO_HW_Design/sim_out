    %assign/vec4 v000001a3ffc424d0_0, 0;
T_3.3 ;
T_3.1 ;
    %jmp T_3;
    .thread T_3;
    .scope S_000001a3ffc2aaa0;
T_4 ;
    %wait E_000001a3ffc28420;
    %load/vec4 v000001a3ffc42890_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v000001a3ffc427f0_0, 0;
    %jmp T_4.1;
T_4.0 ;
    %load/vec4 v000001a3ffc43070_0;
    %load/vec4 v000001a3ffc424d0_0;
    %cmp/u;
    %flag_get/vec4 5;
    %assign/vec4 v000001a3ffc427f0_0, 0;
T_4.1 ;
    %jmp T_4;
    .thread T_4;
    .scope S_000001a3ffc2aaa0;
T_5 ;
    %wait E_000001a3ffc28420;
    %load/vec4 v000001a3ffc42890_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_5.0, 8;
    %pushi/vec4 0, 0, 1;
