    %pushi/vec4 0, 0, 1;
    %assign/vec4 v000001a3ffc42610_0, 0;
    %jmp T_5.1;
T_5.0 ;
    %load/vec4 v000001a3ffc42750_0;
    %cmpi/u 10, 0, 16;
    %flag_or 5, 4; GT is !LE
    %flag_inv 5;
    %jmp/0xz  T_5.2, 5;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v000001a3ffc42570_0, 0;
    %pushi/vec4 0, 0, 1;
